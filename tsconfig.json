{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "*": [
                "node_modules/adobexd-types"
            ]
        },
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src"
    }
}
